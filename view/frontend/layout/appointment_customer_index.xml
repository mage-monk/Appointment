<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
    <update handle="customer_account"/> 
    <body> 
        <referenceBlock name="page.main.title"> 
            <action method="setPageTitle"> 
                <argument translate="true" name="title" xsi:type="string">My Appointments</argument> 
            </action> 
        </referenceBlock> 
        <referenceContainer name="content"> 
            <block class="Deloitte\Appointment\Block\Data" name="my_appointment" template="Deloitte_Appointment::appointments.phtml" cacheable="false" />  
        </referenceContainer> 
    </body> 
</page>
